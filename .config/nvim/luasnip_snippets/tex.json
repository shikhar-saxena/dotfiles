{
  "beg": {
    "prefix": "beg",
    "description": "begin{} / end{}",
    "body": [
      "\\begin{$1\\}",
      "\t$0",
      "\\end{$1\\}"
    ]
  },
  "frame": {
    "prefix": "frame",
    "description": "beamer frame",
    "body": [
      "\\begin{frame}{$1}",
      "$0",
      "\\end{frame}"
    ]
  },
  "/": {
    "prefix": "/",
    "description": "Fraction",
    "body": "\\frac{$1}{$2}$0"
  },
  "b": {
    "prefix": "b",
